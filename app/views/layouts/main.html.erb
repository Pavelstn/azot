      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
      <![endif]-->

     <link rel="shortcut icon" href="/img/favicon.ico">

    </head>

    <body>
  <% if user_signed_in? %>
 <%= render "shared/navbar" %>
  <% end %>
      

    <%= render "shared/top_menu" %>

      <div class="container-fluid">


        <div class="row-fluid">
          <div class="span3">
            <%= render "shared/sidebar-nav" %>
           
            <%= render "shared/banners/left_side_banner2" %>
             <%= render "shared/banners/left_side_banner1" %>
              <%= yield(:partner_banner) %>
          </div><!--/span-->
          <div class="span9 some_page">
              <%= yield %>
          </div><!--/span-->
        </div><!--/row-->

        
      </div>
      <hr>

      <footer>
        <%= render "shared/footer" %>
        <%= render "shared/google_analytics" %>
      </footer>

      <%= render "shared/javascripts" %>
